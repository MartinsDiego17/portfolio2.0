---
import Inicio from "../components/Inicio.astro";
import Experiencia from "../components/Experiencia.astro";
import Formacion from "../components/Formacion.astro";
import Stack from "../components/Stack.astro";
import Proyectos from "../components/Proyectos.astro";
import Contacto from "../components/Contacto.astro";
import Footer from "../components/Footer.astro";
import Layout from "../layouts/Layout.astro";

interface AstroInterface {
  currentLocale: any;
}

import { getI18N } from "../lenguajes";
const { currentLocale }: AstroInterface = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout title={i18n.Layout.title}>
  <Inicio />
  <Experiencia />
  <Formacion />
  <Stack />
  <Proyectos />
  <Contacto />
  <Footer />
</Layout>